{
    "manifest_version": 3,
    "name": "coronavirus word tracker",
    "version": "1.1.0",
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "action": {
        "default_title": "Current status in India",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab"]
}
